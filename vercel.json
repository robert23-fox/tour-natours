{
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "rewrites": [
    { "source": "/tour/:slug", "destination": "/tour/:slug" },
    { "source": "/me", "destination": "/me" },
    { "source": "/my-tours", "destination": "/my-tours" },
    { "source": "/login", "destination": "/login" }
  ],
  "builds": [
    {
      "src": "index.js", 
      "use": "@vercel/node"
    }
  ]
}
